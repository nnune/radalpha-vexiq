r(PORT4, false);\nmotor Bucket = motor(PORT10, false);\n\n\n\n// Generated code.\n\n\n\n// define variable for remote controller enable/disable\nbool RemoteControlCodeEnabled = true;\n\n\n\n// Include the IQ Library\n#include \"vex.h\"\n  \n// Allows for easier use of the VEX Library\nusing namespace vex;\n\nfloat myVariable;\n\n// \"when ToucLed pressed\" hat block\nvoid onevent_ToucLed_pressed_0() {\n  ToucLed.setColor(purple);\n  Drivetrain.driveFor(forward, 19.0, inches);\n  Drivetrain.stop();\n  Drivetrain.turnFor(right, 60.0, degrees);\n  Drivetrain.driveFor(forward, 10.0, inches);\n  Drivetrain.turnFor(right, 47.0, degrees);\n  Drivetrain.driveFor(forward, 5.0, inches);\n  Roller.spin(forward);\n  wait(7.0, seconds);\n  Roller.stop();\n  Drivetrain.driveFor(reverse, 10.0, inches);\n  Drivetrain.turnFor(left, 105.0, degrees);\n  Roller.spin(reverse);\n  BlueDispenser.spinToPosition(750.0, degrees, false);\n  wait(3.0, seconds);\n  Bucket.spinToPosition(1.0, turns, false);\n  wait(0.5, seconds);\n  Bucket.spinToPosition(0.0, turns);\n  wait(0.1, seconds);\n  Bucket.spinToPosition(1.0, turns);\n  wait(6.0, seconds);\n  Drivetrain.driveFor(forward, 19.0, inches);\n  Roller.stop();\n  ToucLed.setColor(yellow);\n  Drivetrain.turnFor(left, 90.0, degrees);\n  Drivetrain.driveFor(forward, 3.0, inches);\n  Drivetrain.turnFor(right, 90.0, degrees);\n  Drivetrain.driveFor(reverse, 0.5, inches);\n  ToucLed.setColor(green);\n}\n\n// \"when started\" hat block\nint whenStarted1() {\n  Roller.setVelocity(250.0, rpm);\n  BlueDispenser.setVelocity(100.0, percent);\n  BlueDispenser.setPosition(0.0, degrees);\n  Drivetrain.setDriveVelocity(50.0, percent);\n  return 0;\n}\n\n\nint main() {\n  // register event handlers\n  ToucLed.pressed(onevent_ToucLed_pressed_0);\n\n  wait(15, msec);\n  whenStarted1();\n}","target":"Physical"}